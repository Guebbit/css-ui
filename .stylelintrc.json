{
  "customSyntax": "postcss-scss",
  "plugins": [
    "stylelint-scss"
  ],
  "reporters": [
    {
      "formatter": "stylelint-mocha-formatter"
    }
  ],
  "extends": [
    "stylelint-config-standard-scss"
  ],
  "rules": {
    "scss/no-global-function-names": null,
    "scss/double-slash-comment-empty-line-before": null,
    "max-line-length": 250
  }
}
