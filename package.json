{
  "name": "@guebbit/scss-library",
  "version": "1.0.2",
  "description": "SCSS library",
  "main": "webpack.config.js",
  "scripts": {
    "test": "mocha",
    "lint": "stylelint \"**/*.scss\" --fix",
    "build": "webpack",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "prepublish": "npm run lint && npm run test && npm run build && npm run docs:build",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "breakpoint-sass": "^3.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.14",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.1.0",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.0.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "vitepress": "^1.0.0-alpha.32",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guebbit/scss-library.git"
  },
  "keywords": [
    "SCSS",
    "Library"
  ],
  "author": {
    "name": "Guebbit",
    "email": "guerzoni.andrea91@gmail.com",
    "url": "https://guebbit.com/"
  },
