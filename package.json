{
  "name": "@guebbit/scss-library",
  "type": "module",
  "version": "1.0.6",
  "description": "SCSS library",
  "main": "vite.config.js",
  "scripts": {
    "test": "mocha",
    "lint": "stylelint \"**/*.scss\" --fix",
    "build": "vite build --out-dir dist",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "prepublish": "npm run lint && npm run test && npm run build && npm run docs:build",
    "publish:public": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "autoprefixer": "^10.4.16",
    "breakpoint-sass": "^3.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "mocha": "^10.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-prefix-selector": "^1.16.0",
    "sass": "^1.69.3",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "vite": "^4.4.11",
    "vitepress": "^1.0.0-rc.22"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guebbit/scss-library.git"
  },
  "keywords": [
    "SCSS",
    "Library"
  ],
  "author": {
    "name": "Guebbit",
    "email": "guerzoni.andrea91@gmail.com",
    "url": "https://guebbit.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Guebbit/scss-library/issues"
  },
  "homepage": "https://github.com/Guebbit/scss-library#readme"
}
